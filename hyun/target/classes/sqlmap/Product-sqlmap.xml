<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="product">

<select id="productList" resultType="ProductVo">
SELECT
	P_NO
	,P_NAME
	,P_GENDER
	,P_KIND
	,P_PRICE
	,P_SALE
	,P_VACCINE
	,P_BIRTH
	,P_IMG
	,P_REGDATE
	FROM PRODUCT
</select>

</mapper>
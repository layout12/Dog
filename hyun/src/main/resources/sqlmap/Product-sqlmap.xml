<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="product">

<select id="productList" resultType="ProductVo">
SELECT
	 PD_NO
	,PD_NAME
	,PD_GENDER
	,PD_KIND
	,PD_PRICE
	,PD_SALE
	,PD_VACCINE
	,PD_BIRTH
	,PD_IMG
	,PD_REGDATE
	FROM PRODUCT
</select>

</mapper>